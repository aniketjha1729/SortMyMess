        <div class="col-md-3 offset-md-1"><button id="myBtn1" onclick="showmodal()" class="btn btn2">Show <i class="fas fa-eye"></i></button></div>
        <div class="col-md-3 offset-md-1"><button id="myBtn2" onclick="calculatemodal()" class="btn btn1">Calculate <i class="fas fa-calculator"></i></button></div>
    </div>
    <br><br>
    <div class="container thumbnail table-responsive-md" style="overflow-x:auto;">
        <table class="table table-bordered">
            <thead class="thead-dark">
                <tr>
                    <th>Name</th>
                    <th>Email</th> 
                </tr>
            </thead>
            <tbody>
                {{#each memberUser}}
                <tr>
                    <td>{{name}}</td>
                    <td>{{email}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table> 
    </div>
