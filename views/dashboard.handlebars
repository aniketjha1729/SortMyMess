{{#each dataUser}}
    <div class="nameId" style="display:none">{{nameId}}</div>
    <div class="item" style="display:none">{{item}}</div>
    <div class="price" style="display:none">{{price}}</div>
{{/each}}
<br><br>
<div class="container">
    <div style="overflow-x:auto;" id="data">
    </div>
</div>
<div id="calculate" class="modal thumbnail">
    <div class="modal-content2">
        <span class="close3">&times;</span>
        <div class="col-md-12">
            <table class="table">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Total Expense</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each memberTotal}}
                    <tr>
                        <td>{{name}}</td>
                        <td>{{values}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
<div id="edit" class="modal">
    <div class="modal-content">
        <span class="close2">&times;</span>
        <form action="/edit" method="POST" class="box">
            <center>
                <div class="form-group thumbnail">
                    <label for="Item Name">Item Name</label>
                    <input type="text" class="form-control" placeholder="Item Name" name="itemName">
                    <input type="hidden" name="idUser" value="{{email}}">
                </div>
                <div class="form-group thumbnail">
                    <label for="Price">Price</label>
                    <input type="number" class="form-control"  placeholder="Price" name="itemPrice">
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </center>
        </form>
    </div>
</div>